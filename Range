#include <stdio.h>

int main() {
    int n,r,min=9,max=0;
    printf("enter a number:",n);
    scanf("%d",&n);
    while(n!=0)
    {
        r=n%10;
        if(r>max)
        
            max=r;
    
        
        if(r<min)
        
            min=r;
    
        n=n/10;
        
    }
        printf("%d",max-min);
    
    return 0;
}
